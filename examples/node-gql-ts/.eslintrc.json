{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "comma-dangle":0,
        "quotes": [2, "double"],
        "semi": [2, "always"],
        "space-before-function-paren": 0
    },
    "root": true,
    "ignorePatterns": "generated.ts"
}
